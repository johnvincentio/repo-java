select varchar(username, 10) "User", varchar(password,10) "Password", varchar(active,1) "Act.", varchar(approval,1) "Approved", 
	varchar(temporarypassword,1) "Temp", failurecounter "Fail#", varchar(question,15) "Question", 
	varchar(answer,15) "answer" from hercmigrationdb.LDAPMembers order by username;

select username, active, question 
from hercmigrationdb.LDAPMembers order by username;

