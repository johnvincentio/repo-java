create table Hercdb.LDAPMembers (
	username varchar(128) not null Constraint pk_LDAPMembers primary key, 
	active varchar(1) not null,
	question varchar(254), 	
	


select varchar(username, 10) "User", varchar(password,10) "Password", varchar(active,1) "Act.", 
varchar(approval,1) "Approved" 
from hercmigrationdb.LDAPMembers 
where username like '1%' 
order by username;

