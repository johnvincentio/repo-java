<?xml version="1.0" encoding="UTF-8"?>

<!--  
Please note:
	& must be converted to &amp;
 -->
 
<loadtest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<stages server="www.kvalentine.com" port="80" contextroot="herc" protocol="https">
		<stage url="index.jsp" description="Home page"/>
		<stage url="login/index.jsp?targetPage=loginView.jsp" description="Login page"/>
		<stage url="handlers/LoginHandler" description="Home page">
			<form name="user" value="aaa"/>
			<form name="password" value="aaa"/>
		</stage>		
		<stage url="rentals/index.jsp?targetPage=catalogView.jsp&amp;catalogType=none" 
			description="Rentals Catalog"/>

		<stage url="rentals/index.jsp?targetPage=categoryView.jsp&amp;catalogType=none&amp;categoryId=1" 
			description="Rentals Catalog - Subcategory"/>

		<stage url="rentals/index.jsp?targetPage=equipmentDetailsView.jsp&amp;subcategory2Id=3&amp;subcategory1Id=2&amp;catalogType=none&amp;categoryId=1" 
			description="Rentals Catalog - List Equipment"/>
			
		<stage url="handlers/RentalsEquipmentDetailsHandler" description="Adding items to the rentals cart">
			<form name="selected4870480" value="on"/>
			<form name="quantity4870480" value="1"/>
			<form name="startDay4870480" value="1"/>
			<form name="startMonthYear4870480" value="072007"/>
			<form name="endDay4870480" value="17"/>
			<form name="endMonthYear4870480" value="112007"/>
		</stage>

		<stage url="handlers/RentalsCartHandler" description="Rentals Cart Checkout">
			<form name="jobLocationName" value="Hertz NJ"/>

			<form name="customerPrefix" value="MR"/>
			<form name="customerFirstName" value="Billaaa"/>
			<form name="customerMiddleName" value="E"/>
			<form name="customerLastName" value="Payaaa"/>
			<form name="customerSuffix" value="SR"/>

			<form name="businessName" value="Pontvale Cleaners"/>
			<form name="businessAddress1" value="413 First Ave"/>
			<form name="businessAddress2" value=""/>
			<form name="businessCity" value="Pontvale"/>
			<form name="businessCountryStateProvince" value="USNJ"/>
			<form name="businessPostalCode" value="03124"/>
			<form name="businessEmail1" value="billpay@yahoo.com"/>
			<form name="businessEmail2" value="billpay@yahoo.com"/>
			<form name="businessPhone" value="912-344-5643"/>			
			<form name="businessFax" value="9435256545"/>
			<form name="comments" value=""/>

			<form name="checkout" value="Submit Query"/>
		</stage>

		<stage url="home/index.jsp?targetPage=homeView.jsp" description="Home Page" output="browser"/>
	</stages>
</loadtest>

